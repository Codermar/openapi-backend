{
  "name": "openapi-backend",
  "description": "Build, Validate, Route, and Mock using OpenAPI specification. Framework-agnostic",
  "version": "1.3.8",
  "author": "Viljami Kuosmanen <viljami@avoinsorsa.fi>",
  "license": "MIT",
  "keywords": [
    "openapi",
    "swagger",
    "router",
    "validation",
    "mock",
    "express",
    "hapi",
    "koa",
    "serverless",
    "lambda",
    "azure"
  ],
  "homepage": "https://github.com/anttiviljami/openapi-backend",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anttiviljami/openapi-backend.git"
  },
  "bugs": {
    "url": "https://github.com/anttiviljami/openapi-backend/issues"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "*.js",
    "*.d.ts",
    "**/*.js",
    "**/*.d.ts",
    "!scripts",
    "!node_modules",
    "!examples",
    "!src",
    "!*.config.js"
  ],
  "dependencies": {
    "@types/lodash": "^4.14.117",
    "@types/swagger-parser": "^4.0.2",
    "ajv": "^6.5.4",
    "cookie": "^0.3.1",
    "lodash": "^4.17.11",
    "openapi-schema-validation": "^0.4.2",
    "openapi-types": "^1.3.2",
    "query-string": "^6.2.0",
    "swagger-parser": "^6.0.2"
  },
  "devDependencies": {
    "@types/cookie": "^0.3.1",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.2",
    "@types/query-string": "^6.1.1",
    "jest": "^23.6.0",
    "markdown-toc": "^1.2.0",
    "prettier": "^1.14.3",
    "source-map-support": "^0.5.9",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.1.6"
  },
  "scripts": {
    "build": "tsc",
    "watch-build": "tsc -w",
    "lint": "tslint --format prose --project .",
    "prettier": "prettier --write src/** __tests__/**",
    "toc": "./scripts/generate-toc.js",
    "test": "NODE_ENV=test jest"
  }
}
